BEGIN ACBRE25P

IF ~Global("ACBREINPARTY","GLOBAL",3)~ THEN BEGIN REJOIN
SAY @0
++ @1 + REJOIN.1
++ @2 + REJOIN.2
END

IF ~~ THEN BEGIN REJOIN.1
SAY @3
IF ~~ THEN DO ~SetGlobal("ACBREINPARTY","GLOBAL",2)JoinParty()~ EXIT
END

IF ~~ THEN BEGIN REJOIN.2
SAY @4
IF ~~ THEN EXIT
END

IF ~Global("ACBREINPARTY","GLOBAL",2)Global("ACBREBROKE","GLOBAL",0)~ THEN BEGIN KICKOUT
SAY @5
IF ~AreaCheck("AR4500")~ THEN REPLY @6 + KICKOUT.1
IF ~!AreaCheck("AR4500")~ THEN REPLY @6 + KICKOUT.2
++ @7 + REJOIN.1
END

IF ~~ THEN BEGIN KICKOUT.1
SAY @8
IF ~~ THEN DO ~SetGlobal("ACBREINPARTY","GLOBAL",3)MoveToPointNoInterrupt([2562.1376])Face(3)~ EXIT
END

IF ~~ THEN BEGIN KICKOUT.2
SAY @9
IF ~~ THEN EXIT
END